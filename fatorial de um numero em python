fatorial = 1

numero = int(input("insira um numero: "))

if numero < 0:
    print("nao existe fatorial de numeros negativos")
elif numero == 0:
    print(f"O fatorial de {numero} eh 1")
else:
    for x in range (1, numero+1):
        fatorial = fatorial*x
    print(f"O fatorial de {numero} eh: {fatorial}")
