chave = input("digite sua senha a ser codificada: ")

senha = ""
for letra in chave:
    if letra in "Aa": senha = senha + "D"
    elif letra in "Bb": senha = senha + "E"
    elif letra in "Cc": senha = senha + "F"
    elif letra in "Dd": senha = senha + "G"
    elif letra in "Ee": senha = senha + "H"
    elif letra in "Ff": senha = senha + "I"
    elif letra in "Gg": senha = senha + "J"
    elif letra in "Hh": senha = senha + "K"
    elif letra in "Ii": senha = senha + "L"
    elif letra in "Jj": senha = senha + "M"
    elif letra in "Kk": senha = senha + "N"
    elif letra in "Ll": senha = senha + "O"
    elif letra in "Mm": senha = senha + "P"
    elif letra in "Nn": senha = senha + "Q"
    elif letra in "Rr": senha = senha + "U"
    elif letra in "Ss": senha = senha + "V"
    elif letra in "1": senha = senha + "#"
    elif letra in "2": senha = senha + "!"
    elif letra in "3": senha = senha + "@"
    elif letra in "4": senha = senha + "$"
    elif letra in "5": senha = senha + "~"
    elif letra in "6": senha = senha + "^"
    elif letra in "7": senha = senha + "{"
    elif letra in "8": senha = senha + "="
    elif letra in "9": senha = senha + "_"
    else: senha = senha + letra
print(senha)
